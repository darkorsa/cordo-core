{
    "name": "darkorsa/cordo-core",
    "description": "Core libraries for Cordo microframework",
    "license": "MIT",
    "require": {
        "beberlei/assert": "^2.6",
        "bshaffer/oauth2-server-php": "^1.10",
        "composer/composer": "^2.2",
        "doctrine/collections": "^1.6",
        "doctrine/dbal": "^2.12",
        "doctrine/orm": "^2.8",
        "doctrine/persistence": "^2.1",
        "filp/whoops": "^2.3",
        "guzzlehttp/psr7": "^2.7",
        "hassankhan/config": "^3.2",
        "illuminate/database": "^10.0",
        "illuminate/events": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/log": "^10.0",
        "illuminate/queue": "^10.0",
        "illuminate/redis": "^10.0",
        "illuminate/translation": "^10.0",
        "illuminate/validation": "^10.0",
        "laminas/laminas-mail": "^2.10",
        "laminas/laminas-permissions-acl": "^2.5",
        "laminas/laminas-servicemanager": "^3.4",
        "league/fractal": "^0.20.0",
        "league/plates": "^3.3",
        "league/tactician": "^1.0",
        "league/tactician-command-events": "^0.6.0",
        "league/tactician-container": "^2.0",
        "league/tactician-doctrine": "^1.1",
        "league/tactician-logger": "^0.11.0",
        "monolog/monolog": "^3.8",
        "myclabs/php-enum": "^1.7",
        "nette/php-generator": "^3.3",
        "nikic/fast-route": "^1.3",
        "php-di/php-di": "^6.0",
        "psr/http-server-middleware": "^1.0",
        "ramsey/uuid": "^4.2",
        "ramsey/uuid-doctrine": "^1.5",
        "relay/relay": "2.x@dev",
        "rollbar/rollbar": "^4.1",
        "symfony/cache": "^6.3",
        "symfony/console": "^6.4",
        "symfony/property-access": "^6.1",
        "symfony/translation": "^6.0",
        "symfony/yaml": "^6.0",
        "tuupola/cors-middleware": "^1.2",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "dev-master",
        "phpstan/phpstan": "^1.12",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Cordo\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "phpstan analyse -l 5 -c phpstan.neon src tests",
        "insights": "phpinsights -v"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
